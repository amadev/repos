#!/usr/bin/env joker
;; #-*- mode: clojure -*-

(ns script
  (:require [joker.os :as os])
  (:require [src.repos.core :as r]))

(try
  (r/main)
  (catch Error e
    (if (ex-message e)
      (printf "Error: %s\nCause: %s\nTry verbose for details\n"
              (ex-message e)
              (:cause (ex-data e)))
      (printf "Error: %s\n" e))
    (os/exit 1)))

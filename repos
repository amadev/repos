#!/usr/bin/env joker
;; #-*- mode: clojure -*-

(ns script
  (:require [joker.os :as os])
  (:require [src.repos.core :as r]))

(try
  (r/main)
  (catch Error e
    (r/error "Error: %s\nCause: %s\nTry verbose for details\n"
             (ex-message e)
             (:cause (ex-data e)))
    (os/exit 1)))
